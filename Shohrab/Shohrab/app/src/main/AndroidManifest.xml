<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="finegold.com.shohrab">

    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <permission
        android:name="finegold.com.activity.MainActivity"
        android:label="@string/permisstion_title_contact"
        android:description="@string/permission_desc_contact"
        android:permissionGroup="android.permission-group.CONTACTS"
        android:protectionLevel="dangerous" />
    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="finegold.com.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
